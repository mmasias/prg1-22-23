public class retoprogra {
    public static void main(String[]args) {
        System.out.print("Reto numero 1:");
        System.out.println("\n");
        System.out.println("**********");
        System.out.println("**********");
        System.out.println("**********");
        System.out.println("**********");
        System.out.println("**********");
        System.out.println("\n");

        System.out.print("Reto numero 2:");
        System.out.println("\n");
        System.out.println("**  **  **");
        System.out.println("**  **  **");
        System.out.println("  **  **  ");
        System.out.println("  **  **  ");
        System.out.println("**  **  **");
        System.out.println("**  **  **");
        System.out.println("\n");

        System.out.print("Reto numero 3:");
        System.out.println("\n");
        System.out.println("+--------------------+");
        System.out.println("|**  **  **  **  **  |");
        System.out.println("|**  **  **  **  **  |");
        System.out.println("|  **  **  **  **  **|");
        System.out.println("|  **  **  **  **  **|");
        System.out.println("|**  **  **  **  **  |");
        System.out.println("|**  **  **  **  **  |");
        System.out.println("|  **  **  **  **  **|");
        System.out.println("|  **  **  **  **  **|");
        System.out.println("+--------------------+");
        System.out.println("\n");

        System.out.print("Reto numero 4:");
        System.out.println("\n");
        System.out.println("    *");
        System.out.println("   ***");
        System.out.println("  *****");
        System.out.println(" *******");
        System.out.println("********");
        System.out.println("*********");
        System.out.println("*********");
        System.out.println("*********");
        System.out.println("*********");
        System.out.println("*********");
        System.out.println("\n");

        System.out.print("Reto numero 5:");
        System.out.println("\n");
        System.out.println("    *");
        System.out.println("   ***");
        System.out.println("  *****");
        System.out.println(" *******");
        System.out.println("********");
        System.out.println("*********");
        System.out.println("*********");
        System.out.println("***   ***");
        System.out.println("***   ***");
        System.out.println("*********");
        System.out.println("\n");
}
}
